<center>
<h1>Login</h1>

<%= form_with(scope: :session, url: login_path) do |form| %>

<div>
  <%= form.label :Email %></br>
  <%= form.email_field :email %>
</div>

<div>
  <%= form.label :Password %></br>
  <%= form.password_field :password %>

</div>

<div>
  <%= form.submit "Login"%>
</div>
<% end %>
</center>
